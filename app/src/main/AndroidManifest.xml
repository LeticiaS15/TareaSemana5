<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyecto_petagram40">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_hueso"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.proyecto_petagram40.menu.About"
            android:parentActivityName="com.example.proyecto_petagram40.MainActivity"/>
        <activity
            android:name="com.example.proyecto_petagram40.menu.Contacto"
            android:parentActivityName="com.example.proyecto_petagram40.MainActivity" />
        <activity
            android:name="com.example.proyecto_petagram40.MascotasFavoritas"
            android:parentActivityName="com.example.proyecto_petagram40.MainActivity" />
        <activity android:name="com.example.proyecto_petagram40.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>
